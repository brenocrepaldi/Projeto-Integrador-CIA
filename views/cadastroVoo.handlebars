<div class="container-cadastro">
  <div class="header-cadastro">
    <h1>Cadastro de Voo</h1>
  </div>
  <form action="/cadastro/voo" method="POST" class="form">
    <div class="control-input">
      <div class="input">
        <label for="preco">Pre√ßo</label>
        <input
          type="text"
          name="preco"
          placeholder="Insira o modelo da aeronave"
        />
      </div>
      <div class="input">
        <label for="idTrecho">Trecho</label>
        <select name="idTrecho">
          {{#each trechos}}
            <option
              value="{{this.idaeroportochegada}}"
            >{{this.idaeroportosaida}}</option>
          {{/each}}
        </select>
      </div>
    </div>
    <div class="control-button">
      <button type="submit">Cadastrar</button>
      <button type="reset">Limpar</button>
    </div>
  </form>
</div>