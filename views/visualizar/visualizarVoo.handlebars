<div class="container-visualizar">
  <div class="header-visualizar">
    <h1>Voos</h1>
  </div>
  <div class="control-dados table-responsive">
    <table id="example" class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Código</th>
          <th>Preço</th>
          <th>Data de Saída</th>
          <th>Data de Chegada</th>
          <th>Trecho</th>
          <th>Aeronave</th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        {{#each voos}}
          <tr data-id="{{this.idVoo}}">
            <td>{{this.idVoo}}</td>
            <td>R$ {{this.valor}}</td>
            <td>{{this.horaSaida}}</td>
            <td>{{this.horaChegada}}</td>
            <td>{{this.idTrecho}}</td>
            <td>{{this.idAeronave}}</td>
            <td class="button"><button><a
                  href="/editar/voo/{{this.idVoo}}"
                >Editar</a></td>
            <td class="button">
              <form
                action="/excluir/voo/{{this.idVoo}}"
                method="POST"
              ><button type="submit">Excluir</button></form></td>
          </tr>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>